{
  "name": "express-sequelize-boilerplate",
  "version": "1.0.0",
  "description": "Express Sequelize Boilerplate",
  "main": ".eslintrc.js",
  "scripts": {
    "start": "node ./app/server.js",
    "start-dev": "./node_modules/.bin/nodemon ./app/server.js",
    "test": "./node_modules/mocha/bin/mocha --exit --reporter spec --timeout 12000 --require ./app/server.js test/*.js",
    "create-db": "node_modules/.bin/sequelize db:create --config=app/db/datasource/Mysql.js",
    "migrate": "node_modules/.bin/sequelize db:migrate --migrations-path=app/db/migrations --config=app/db/datasource/Mysql.js",
    "seed": "./node_modules/.bin/sequelize db:seed:all --seeders-path=app/db/seeds --config=app/db/datasource/Mysql.js",
    "eslint-fix": "./node_modules/.bin/eslint --fix ."
  },
  "keywords": [
    "express"
  ],
  "pre-commit": [
    "eslint-fix",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panduakas/express-sequelize-boilerplate.git"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "axios-retry": "^3.1.8",
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "cuid": "^2.1.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-enrouten": "^1.3.0",
    "express-paginate": "^1.0.0",
    "express-rate-limit": "^5.0.0",
    "express-validator": "^5.3.1",
    "express-winston": "^3.0.1",
    "faker": "^4.1.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^4.4.1",
    "retry-axios": "^2.2.1",
    "sequelize": "^5.9.4",
    "sequelize-cli": "^5.5.0",
    "swagger-jsdoc": "^3.2.3",
    "swagger-ui-express": "^4.0.1",
    "winston": "^2.4.1",
    "winston-daily-rotate-file": "^3.4.1",
    "winston-logstash": "^0.4.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "sequelize-auto": "^0.4.29",
    "stylelint": "^9.10.1",
    "supertest": "^3.4.2"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/panduakas/express-sequelize-boilerplate/issues"
  },
  "homepage": "https://github.com/panduakas/express-sequelize-boilerplate#readme"
}
